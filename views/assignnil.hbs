<link rel="stylesheet" type="text/css" href="/stylesheets/class.css">
<div id="wrapper" class="toggled">
   <!-- Sidebar -->
        <div id="sidebar-wrapper">
            <ul class="sidebar-nav">
                <li class="sidebar-brand">
                    <h5 class="sidebar-title">
                      {{classRoom.name}}
                  </h5>
                </li>
                <li>
                    <a href="/home"><h3>Home</h3></a>
                </li>
                 <li>
                    <a href="/classRoom/{{classRoom._id}}"><h3>Assignments</h3></a>
                </li>
                <li>
                   <a href="/classRoomBalance/{{classRoom._id}}"><h3>Balances</h3></a>
                </li>
                 <li>
                   <a href="/charts/{{classRoom._id}}"><h3>Graphs</h3></a>
                </li>
                <li>
                   <a href="/market_settings/{{classRoom._id}}"><h3>Market Settings</h3></a>
                </li>
                <li>
                   <a href="/logout"><h3>Logout</h3></a>
                </li>
            </ul>
